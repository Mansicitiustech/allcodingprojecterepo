<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
<bean id="person" class="com.springcore.entity.Person" autowire="byType">
<property name="id" value="10"></property>
<property name="name" value="shreya"></property>
</bean>

<bean id="address1" class="com.springcore.entity.Address">
<property name="addid" value="111"></property>
<property name="city" value="indore"></property>
</bean>



</beans>
