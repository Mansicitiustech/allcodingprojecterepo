<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

<bean id="student" class="com.springcore.entity.Student">
<property name="sid"><value>222</value></property>
<property name="name"><value>arpita</value></property>
<property name="address">
<bean class="com.springcore.entity.Address">
<property name="addid"><value>55</value></property>
<property name="city"><value>bhopal</value></property>
</bean>
</property>
<property name="teacher">
<bean class="com.springcore.entity.Teacher">
<property name="tid"><value>65</value></property>
<property name="tname"><value>vikrant</value></property>
<property name="tsubject"><value>maths</value></property>
</bean>
</property>

</bean>
</beans>
